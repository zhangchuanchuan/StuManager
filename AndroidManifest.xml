            android:name="com.stream.stumanager.activity.LoginActivity"
            android:label="@string/app_name">
            <intent-filter >
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name="com.stream.stumanager.activity.ClassesActivity"></activity>
        <activity android:name="com.stream.stumanager.activity.MajorActivity"></activity>
        <activity android:name="com.stream.stumanager.activity.RegistActivity"></activity>
        <activity android:name="com.stream.stumanager.activity.StudentActivity"></activity>
